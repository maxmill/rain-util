{
  "name": "rain-util",
  "version": "0.2.5",
  "author": "Max Millien",
  "description": "Generator based swiss knife for consuming apis, accessing postgres, and managing filesystem",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/maxmill/rain-util.git"
