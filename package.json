{
  "name": "rain-util",
  "version": "0.4.6",
  "author": "Max Millien",
  "description": "Generator based swiss knife for consuming apis, accessing postgres, and managing filesystem",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/maxmill/rain-util.git"
  },
  "license": "MIT",
  "main": "./bin",
  "scripts": {
    "clean": "npm cache clean && rm -rf node_modules bin data",
    "build": "babel lib -d bin",
    "lint": "eslint . --fix",
    "postbuild": "npm test",
    "test": "faucet"
  },
  "keywords": [
    "koa",
    "postgres",
    "api",
    "http",
    "fs",
    "json",
    "mkdirp",
    "utils",
    "requests",
    "co",
    "download",
    "generators",
    "bundle",
    "convenience",
    "toolkit",
    "schema"
  ],
  "dependencies": {
    "array-generators": "*",
    "debug": "^2.2.0",
    "co-tape": "*",
    "rain-util-download": "*",
    "rain-util-fs": "*",
    "rain-util-http": "*",
    "rain-util-postgres": "*",
    "tape-catch": "1.x",
    "thunkify-wrap": "1.x"
  },
  "devDependencies": {
    "babel": "6.x",
    "babel-cli": "6.x",
    "babel-preset-node6": "*",
    "eslint": "^3.2.0",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-import": "^1.12.0",
    "faucet": "0.x",
    "tape": "4.x"
  },
  "engines": {
    "node": ">= 6.9.1"
  },
  "babel": {
    "presets": [
      "node6"
    ]
  }
}
