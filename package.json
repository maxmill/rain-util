    "request": "2.x",
    "rimraf": "2.4.x",
    "thunkify-wrap": "1.x"
  },
  "devDependencies": {
    "fly": "*",
    "fly-babel": "0.x",
    "tape": "4.x",
    "tape-catch": "1.0.x"
  },
