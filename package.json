{
  "name": "rain-util",
  "version": "0.4.2",
  "author": "Max Millien",
  "description": "Generator based swiss knife for consuming apis, accessing postgres, and managing filesystem",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/maxmill/rain-util.git"
  },
  "license": "MIT",
  "main": "./bin",
  "scripts": {
    "clean": "npm cache clean && rm -rf node_modules bin data",
    "build": "babel lib -d bin",
    "postbuild":"npm test",
    "test": "faucet"
  },
  "keywords": [
    "koa",
    "postgres",
    "api",
    "http",
    "fs",
    "json",
    "mkdirp",
    "utils",
    "requests",
    "co",
    "download",
    "generators",
    "bundle",
    "convenience",
    "toolkit",
    "schema"
  ],
  "dependencies": {
    "array-generators":"*",
    "rain-util-download": "*",
    "rain-util-fs": "*",
    "rain-util-http": "*",
    "rain-util-postgres": "*",
    "thunkify-wrap": "1.x"
  },
  "devDependencies": {
    "babel": "*",
    "babel-cli": "*",
    "babel-plugin-transform-object-rest-spread": "*",
    "babel-preset-es2015-node5": "*",
    "babel-preset-stage-3": "*",
    "co-tape": "*",
    "faucet": "0.x",
    "tape": "4.x",
    "tape-catch": "1.x"
  },
  "engines": {
    "node": ">= 4.2.0"
  },
  "babel": {
    "presets": [
      "es2015-node5",
      "stage-3"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}
